<div class="print-area"  id="female-print" style="display:none">
    <h2 class="print-head">门诊病历</h2>
    <table class="print-table">
        <tr>
            <td width="33%"><span class="td-title">姓名</span><div class="value">{{info.Name}}</div></td>
            <td width="33%"><span class="td-title">性别</span><div class="value">{{info.Sex}}</div></td>
            <td width="34%"><span class="td-title">病历号</span><div class="value">{{info.No}}</div></td>
        </tr>
        <tr>
            <td><span class="td-title">民族</span><div class="value">{{info.Ethnic}}</div></td>
            <td><span class="td-title">婚姻</span><div class="value">{{info.Marriage}}</div></td>
            <td><span class="td-title">年龄</span><div class="value">{{info.Age}}</div></td>
        </tr>
        <tr>
            <td colspan="3">
                <div style="width:50%;float:left"><span class="td-title">职业</span><div class="value">{{info.Job}}</div></div>
                <div style="width:50%;float:left"><span class="td-title">电话</span><div class="value">{{info.Phone}}</div></div>
            </td>
        </tr>
        <tr>
            <td colspan="3">
                <span class="td-title">证件号</span><div class="value">{{info.CardNo}}</div>
            </td>
        </tr>
        <tr>
            <td colspan="3" style="padding:10px 10px 10px 0">
                <table class="print-detail">
                    <tr>
                        <td width="100px">主诉</td>
                        <td>{{info.MainInfo}}</td>
                    </tr>
                    <tr>
                        <td>现病史</td>
                        <td>{{info.CurrentInfo}}</td>
                    </tr>
                    <tr>
                        <td>既往史</td>
                        <td>{{info.PastInfo}}</td>
                    </tr>
                    <tr>
                        <td>个人史</td>
                        <td>{{info.PersonalInfo}}</td>
                    </tr>
                    <tr>
                        <td>月经史</td>
                        <td>{{info.MenstruationInfo}}</td>
                    </tr>
                    <tr>
                        <td>婚育史</td>
                        <td>{{info.MarriageInfo}}</td>
                    </tr>
                    <tr>
                        <td>体格检查</td>
                        <td>{{info.PhysiqueCheckInfo}}</td>
                    </tr>
                    <tr>
                        <td>妇科检查</td>
                        <td>{{info.GynecologyCheckInfo}}</td>
                    </tr>
                    <tr>
                        <td>诊断</td>
                        <td>{{info.DiagnosisInfo}}</td>
                    </tr>
                    <tr>
                        <td>治疗意见</td>
                        <td>{{info.AdviceInfo}}</td>
                    </tr>

                </table>

            </td>
        </tr>
        <tr>
            <td colspan="3" style="text-align:right">
                <span class="table-bottom">医师：{{info.Doctor}}</span>
                <span class="table-bottom">{{info.Time}}</span>
            </td>
        </tr>

    </table>
</div>



    
  
